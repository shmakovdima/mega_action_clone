mongoose     = require 'mongoose'
config       = require "./config.json"
# database connection
mongoose.connect(config.db.path + config.db.name)
db = mongoose.connection

db.on 'error', (err)->
  console.log "Error connection to database: ", err
db.once 'open', ()->
  console.log "Connected to db!"

  # Schemas loading
  a = 0
  CardModel = require('./schemas/cards.coffee').get mongoose
  saveCard = (card_number, i)->
    model = new CardModel {number: card_number, owner: i}
    model.save (err)->
      console.log(err) if err
      a = a+1
      console.log "that's all!" if a is 950

  for i, card of cards
    for card_number in card
      saveCard card_number, i